                      00212 
                      00213 ; insert melody table multiplexer code (begin a table)
                      00214 MELODY macro name
                      00215 idx SET 0
                      00216 name 
                      00217     movlw high (name+7)
                      00218     movwf PCLATH
                      00219     movlw low (name+7)
                      00220     addwf note_idx,W
                      00221     skpnc
                      00222     incf PCLATH
                      00223     movwf PCL
                      00224     endm
                      00225 
                      00226 ; compute duration value from tempo 
                      00227 ; This macro must be the first entry in melody table after MELODY
                      00228 ; Insert 16 bits data in low, high order
                      00229 ; arguments:    
                      00230 ;   'tempo' is  QUARTER/minute
                      00231 TEMPO macro tempo    
                      00232     dt  low (ENV_CLK*60*4/tempo-1), high (ENV_CLK*60*4/tempo-1)
                      00233 idx SET idx+2
                      00234     endm
                      00235     
                      00236 ; insert end of table code (mark end of table)
                      00237 MELODY_END macro
                      00238     retlw STAFF_END
                      00239     endm
                      00240 
                      00241     
                      00242 ; add staff note entry to melody table, one entry by staff note.
                      00243 ; arguments:
                      00244 ;   'name', note name {C4,C4s,D4f,....} 
                      00245 ;   'time', duration {WHOLE, WHOLE_DOT, HALF,...} 
                      00246 NOTE macro name, time
                      00247     retlw ((time<<4)|name)
                      00248 idx SET idx+1
                      00249     endm
                      00250 
                      00251 ; add a pause entry
                      00252 ;  arguments:
                      00253 ;       'time', duration 
                      00254 PAUSE macro time
                      00255     retlw ((time<<4)|0xf)
